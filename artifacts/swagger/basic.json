  "definitions": {
    "Product": {
      "type": "object",
      "properties": {
        "id": {
          "format": "int32",
          "type": "integer"
        },
        "description": { "type": "string" },
        "status": {
          "format": "int32",
          "enum": [ 0, 1, 2 ],
          "type": "integer"
        }
      }
    },
    "PaymentRequest": {
      "required": [ "transaction", "creditCard" ],
      "type": "object",
      "properties": {
        "transaction": { "$ref": "#/definitions/Transaction" },
        "creditCard": { "$ref": "#/definitions/CreditCard" }
      }
    },
    "Transaction": {
      "required": [ "amount" ],
      "type": "object",
      "properties": {
        "amount": {
          "format": "double",
          "type": "number"
        },
        "note": { "type": "string" }
      }
    },
    "CreditCard": {
      "required": [ "cardNumber", "expMonth", "expYear" ],
      "type": "object",
      "properties": {
        "cardNumber": {
          "pattern": "^[3-6]?\\d{12,15}$",
          "type": "string"
        },
        "expMonth": {
          "format": "int32",
          "maximum": 12,
          "minimum": 1,
          "type": "integer"
        },
        "expYear": {
          "format": "int32",
          "maximum": 99,
          "minimum": 14,
          "type": "integer"
        }
      }
    },
    "Promotion": {
      "type": "object",
      "properties": {
        "promo-code": { "type": "string" },
        "discountType": {
          "enum": [ "Percentage", "Amount" ],
          "type": "string"
        }
      }
    },
    "Cart": {
      "default": { "id": -1 },
      "type": "object",
      "properties": {
        "id": {
          "format": "int32",
          "type": "integer",
          "readOnly": true
        }
      }
    }
  },
